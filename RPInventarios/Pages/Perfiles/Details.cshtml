@page "{id:int?}"
@model RPInventarios.Pages.Perfil.DetailsModel

@{
    ViewData["Title"] = "Detalles del perfil";
}

<h4>@ViewData["Title"]</h4>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Perfil.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Perfil.Nombre)
        </dd>
    </dl>
</div>
<div>
    <div class="row">
        <div class="col">
            <partial name="Acciones/_Details" model="@Model.Perfil.Id" />
        </div>
    </div>
</div>
